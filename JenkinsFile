pipeline {
    agent any

    stages {
        stage('Hello1') {
            steps {
                echo 'Hello World1'
            }
        }
         
         stage('Build1') {
            steps {
                echo 'Building1'
            }
        }
         stage('Deploy1') {
            steps {
                echo 'Deploying1'
            }
        }
        stage('Test1') {
            steps {
                echo 'Testing1'
            }
        }
        stage('Release1') {
            steps {
                echo 'Releasing1'
            }
        }
    }
}
